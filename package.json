{
  "name": "n8n-nodes-directus",
  "version": "2.0.0",
  "description": "Node to interact with a Directus instance using the latest SDK",
  "keywords": [
    "n8n-community-node-package",
    "directus",
    "cms",
    "headless",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/arladmin/n8n-nodes-directus",
  "author": {
    "name": "n8n-nodes-directus",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arladmin/n8n-nodes-directus.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DirectusApi.credentials.js",
      "dist/credentials/DirectusOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Directus/Directus.node.js"
    ]
  },
  "dependencies": {
    "@directus/sdk": "^20.0.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-core": "^1.21.0",
    "n8n-workflow": "^1.21.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}