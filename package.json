{
  "name": "n8n-nodes-directus",
  "version": "2.0.0",
  "description": "Node to interact with a Directus instance using the latest SDK",
  "keywords": [
    "n8n-community-node-package",
    "directus",
    "cms",
    "headless",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/arladmin/n8n-nodes-directus",
  "author": {
    "name": "n8n-nodes-directus",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arladmin/n8n-nodes-directus.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "format:check": "prettier nodes credentials --check",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "lint:check": "eslint nodes credentials --ext .ts",
    "test": "jest",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:integration": "jest --config jest.config.integration.js --testPathPattern=test/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint:check && npm test"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DirectusApi.credentials.js",
      "dist/credentials/DirectusOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Directus/Directus.node.js"
    ]
  },
  "dependencies": {
    "@directus/sdk": "^20.0.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "^1.21.0",
    "n8n-workflow": "^1.21.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}